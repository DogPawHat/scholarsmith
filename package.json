{
    "name": "scholarsmith-react",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "webpack -d",
        "pretest": "tsc",
        "test": "ava --tap | tap-nyan",
        "cover": "nyc npm run test",
        "build": "node dist/builder.bundle.js",
        "start": "webpack-dev-server -d --content-base dist/"
    },
    "ava": {
        "require": [
            "babel-register",
            "./test/helpers/setup-test-env.js"
        ],
        "exclude": [
            "./test/helpers/**/*",
            "./test/test_lib/**/*"
        ],
        "babel": "inherit"
    },
    "nyc": {
        "require": [
            "babel-register"
        ],
        "extension": [
            ".jsx"
        ],
        "reporter": [
            "lcov",
            "text",
            "html"
        ],
        "all": true,
        "exclude": [
            "test/**/*",
            "dist/**/*",
            "coverage/**/*",
            "webpack.config.js"
        ],
        "sourceMap": true
    },
    "author": "",
    "license": "UNLICENCED",
    "devDependencies": {
        "ava": "^0.16.0",
        "awesome-typescript-loader": "^1.1.1",
        "axios": "^0.13.1",
        "babel-core": "^6.11.4",
        "babel-loader": "^6.2.4",
        "babel-plugin-istanbul": "^1.0.3",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.11.6",
        "browser-sync": "^2.13.0",
        "classnames": "^2.2.5",
        "css-loader": "^0.23.1",
        "enzyme": "^2.4.1",
        "foundation-sites": "^6.2.3",
        "immutable": "^3.8.1",
        "js-yaml": "^3.6.1",
        "jsdom": "^9.4.1",
        "marked": "^0.3.5",
        "mocha": "^3.0.2",
        "node-sass": "^3.8.0",
        "nyc": "^7.1.0",
        "react": "^15.3.0",
        "react-addons-test-utils": "^15.3.0",
        "react-dom": "^15.3.0",
        "react-redux": "^4.4.5",
        "react-router": "^2.6.0",
        "redux": "^3.5.2",
        "redux-immutable": "^3.0.6",
        "sass-loader": "^4.0.0",
        "style-loader": "^0.13.1",
        "tap-nyan": "0.0.2",
        "tape": "^4.6.0",
        "ts-loader": "^0.8.2",
        "ts-node": "^1.2.2",
        "typescript": "^1.8.10",
        "webpack": "^1.13.1",
        "webpack-dev-server": "^1.14.1",
        "yaml-front-matter": "^3.4.0"
    }
}