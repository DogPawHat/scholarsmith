  "author": {
    "name": "Ciar√°n Curley",
    "email": "ciaran.curley.11.11@gmail.com"
  },
  "description": "Static Generator of E-learning content",
  "main": "index.js",
  "scripts": {
    "prebuild": "webpack -d",
    "build": "node dist/builder.bundle.js",
    "start": "webpack-dev-server -d --content-base dist/",
    "lint": "tslint -c tslint.json --test ./src",
    "test": "tape -r ts-node/register ./test/*.spec.ts",
    "cover": "nyc yarn test"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".tsx"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "all": true,
    "exclude": [
      "src/templates/container/ActiveQuestionPage.tsx",
      "test/**/*",
      "dist/**/*",
      "coverage/**/*",
      "webpack.config.js"
    ],
    "sourceMap": true
  },
  "license": "MIT",
