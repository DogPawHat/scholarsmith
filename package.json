{
  "name": "scholarsmith",
  "version": "2.4.1",
  "repository": {
    "url": "https://github.com/DogPawHat/scholarsmith",
    "type": "git"
  },
  "author": {
    "name": "Ciar√°n Curley",
    "email": "ciaran.curley.11.11@gmail.com"
  },
  "description": "Static Generator of E-learning content",
  "main": "index.js",
  "scripts": {
    "prebuild": "webpack -d",
    "build": "node dist/builder.bundle.js",
    "start": "webpack-dev-server -d --content-base dist/",
    "lint": "tslint -c tslint.json --test ./src",
    "test": "ts-node tape ./test/*.spec.ts",
    "cover": "nyc tape -r ts-node/register ./test/*.spec.ts"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ],
    "all": true,
    "exclude": [
      "node_modules/**/*",
      "test/**/*",
      "dist/**/*",
      "coverage/**/*",
      "webpack.config.js",
      "src/yaml-front-matter.d.ts"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.9.32",
    "@types/enzyme": "^2.4.36",
    "@types/js-yaml": "^3.5.28",
    "@types/jsdom": "^2.0.29",
    "@types/marked": "0.0.28",
    "@types/mz": "^0.0.30",
    "@types/node": "^6.0.45",
    "@types/react": "^0.14.39",
    "@types/react-dom": "^0.14.17",
    "@types/react-redux": "^4.4.32",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/redux-immutable": "^3.0.31",
    "@types/systemjs": "^0.19.32",
    "@types/tape": "^4.2.28",
    "axios": "^0.15.0",
    "css-loader": "latest",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.9.0",
    "foundation-sites": "^6.2.3",
    "immutable": "^3.8.1",
    "js-yaml": "^3.6.1",
    "jsdom": "^9.4.1",
    "marked": "^0.3.5",
    "mz": "^2.6.0",
    "node-sass": "^4.1.0",
    "nyc": "^7.1.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-form": "^6.2.0",
    "redux-immutable": "^3.0.6",
    "sass-loader": "^4.0.0",
    "static": "^2.0.0",
    "static-site-generator-webpack-plugin": "^3.1.0",
    "style-loader": "^0.13.1",
    "tape": "^4.6.0",
    "ts-loader": "latest",
    "ts-node": "^1.3.0",
    "tslint": "latest",
    "tslint-loader": "latest",
    "typescript": "^2.0.3",
    "webpack": "beta",
    "webpack-dev-server": "^1.14.1",
    "yaml-front-matter": "^3.4.0"
  }
}
